      Sql.Strings = (
        'SELECT distinct a.MATRMEC,a2.GRADO, a1.COGNOME,a1.NOME,a3.REPARTO,a1.celservizio FROM AUTORIZZAZIONIVESTIARIO a'
        'inner join ANAGRAFICA a1 on (a.MATRMEC = a1.MATRMEC)'
        'inner join GRADI a2 on (a1.KSGRADO = a2.IDGRADI)'
        'inner join REPARTI a3 on (a1.KSREPARTO = a3.IDREPARTO)'
        'order by a3.reparto,a1.cognome,a1.nome'
      )
